\mode<presentation>

%Color
\definecolor{dark-math}{RGB}{80,99,39}
\definecolor{light-math}{RGB}{120,145,61}


% Settings
\setbeamercolor*{title page header}{fg=dark-math}
\setbeamercolor*{author}{fg=black}
\setbeamercolor*{date}{fg=black}


\setbeamercolor{palette primary}{fg=dark-math}
\setbeamercolor{palette secondary}{fg=black}
\setbeamercolor{palette tertiary}{fg=light-math}
\setbeamercolor{palette quaternary}{fg=black}
\setbeamercolor{structure}{fg=dark-math} % itemize, enumerate, etc
\setbeamercolor{section in toc}{fg=light-math} % TOC sections

% Override palette coloring with secondary
\setbeamercolor{subsection in head/foot}{fg=black}

% Items
\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]

\mode
<all>
